{
  "name": "montr",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/**",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:release": "node_modules/.bin/react-native run-android --variant=release",
    "serve": "node_modules/.bin/react-native run-android",
    "build": "cd android && gradlew assembleRelease",
    "timesync:win": "adb shell date -s SET $(get-date -format MMddhhmm.ss)",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "remotedev:port": "adb reverse tcp:8097 tcp:8097",
    "remotedev:inject": "remotedev --hostname=localhost --port=8000 --injectserver=reactnative",
    "remotedev:revert": "remotedev --revert=reactnative"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "react-native": "^0.41.0",
    "react": "16.7.0",
    "react-native-android-statusbar": "^0.1.3",
    "react-native-material-design": "^0.3.7",
    "react-native-material-design-styles": "^0.2.6",
    "react-redux": "^5.0.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.0",
    "redux-devtools-extension": "^2.12.2",
    "redux-thunk": "^2.1.0",
    "snyk": "^1.24.4"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-react-native": "^1.9.1",
    "babel-register": "^6.18.0",
    "eslint": "^4.1.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^7.0.0",
    "react-native-cli": "^2.0.0",
    "remote-redux-devtools": "0.5.8",
    "remotedev-server": "^0.3.0-beta-2"
  },
  "snyk": true
}
